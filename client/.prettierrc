{
	"tabWidth": 2,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"useTabs": true,
	"semi": false,
	"bracketSpacing": true,
	"bracketSameLine": true,
	"arrowParens": "always",
	"endOfLine": "lf",
	"trailingComma": "none",
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"/hooks/",
		"/components/",
		"module\\.(scss|css)",
		"^../(.*)",
		"^./(.*)"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
